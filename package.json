{
  "name": "oclif-template",
  "description": "oclif multi command, space separated cli",
  "version": "0.0.1",
  "author": "tiagonapoli",
  "homepage": "https://github.com/tiagonapoli/oclif-template",
  "bugs": "https://github.com/tiagonapoli/oclif-template/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/tiagonapoli/oclif-template.git"
  },
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/oclif.manifest.json"
  ],
  "main": "lib/index.js",
  "bin": {
    "cli": "./bin/run"
  },
  "scripts": {
    "watch": "nodemon",
    "format": "prettier --config ./.prettierrc --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "format-lint": "yarn format && yarn lint",
    "build": "rm -rf lib && tsc && yarn oclif-dev manifest",
    "docs": "yarn oclif-dev readme --spaceSep",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc && yarn oclif-dev manifest && yarn docs",
    "preversion": "yarn docs && git add README.md",
    "test": "jest --passWithNoTests",
    "ci:test": "yarn test --ci"
  },
  "dependencies": {
    "@oclif/command": "^1.0.0",
    "@oclif/config": "^1.0.0",
    "@oclif/plugin-help": "^2.2.3",
    "@oclif/plugin-update": "^1.3.9",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "@tiagonapoli/oclif-plugin-spaced-commands": "^0.0.2",
    "chalk": "^3.0.0",
    "tslib": "^1.0.0"
  },
  "devDependencies": {
    "@tiagonapoli/oclif-dev-cli": "^0.0.3",
    "@types/jest": "^24.0.25",
    "@types/node": "^12.12.21",
    "@typescript-eslint/eslint-plugin": "^2.3.0",
    "@typescript-eslint/parser": "^2.12.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.2.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-lodash": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "globby": "^10.0.0",
    "jest": "^24.9.0",
    "nodemon": "^2.0.2",
    "nodemon-notifier-cli": "https://github.com/Slessi/nodemon-notifier-cli.git",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.0.0",
    "typescript": "^3.7.4"
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "cli",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-update",
      "@oclif/plugin-warn-if-update-available",
      "@tiagonapoli/oclif-plugin-spaced-commands"
    ],
    "topics": {
      "say": {
        "description": "Commands to say something"
      }
    }
  }
}
